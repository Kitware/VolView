{
  "name": "volview",
  "version": "4.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "serve": "npm run dev -- --port 8080",
    "build": "vue-tsc --noEmit && vite build",
    "build:analyze": "cross-env ANALYZE_BUNDLE=1 npm run build",
    "test": "vitest",
    "test:e2e:chrome": "npm run build && wdio run ./wdio.chrome.conf.ts",
    "lint": "eslint",
    "build:all": "npm run build:dicom && npm run build:resample && npm run build",
    "build:dicom": "itk-wasm -s src/io/itk-dicom/ build ",
    "build:dicom:debug": "itk-wasm -s src/io/itk-dicom/ build -- -DCMAKE_BUILD_TYPE=Debug",
    "build:resample": "itk-wasm -s src/io/resample/ build ",
    "build:resample:debug": "itk-wasm -s src/io/resample/ build -- -DCMAKE_BUILD_TYPE=Debug",
    "build:watch": "npm run build -- --watch",
    "postinstall": "patch-package",
    "prettify": "prettier --write src tests",
    "doc": "kw-doc -c ./documentation/config.cjs",
    "doc:www": "kw-doc -c ./documentation/config.cjs -s",
    "doc:publish": "kw-doc -c ./documentation/config.cjs -mp"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.321.1",
    "@kitware/vtk.js": "^27.4.6",
    "@netlify/edge-functions": "^2.0.0",
    "@sentry/vue": "^7.54.0",
    "@vueuse/core": "^8.5.0",
    "@vueuse/shared": "^9.2.0",
    "core-js": "3.22.5",
    "deep-equal": "^2.0.5",
    "dicomweb-client-typed": "^0.8.6",
    "file-saver": "^2.0.5",
    "gl-matrix": "3.4.3",
    "itk-image-io": "1.0.0-b.66",
    "itk-wasm": "1.0.0-b.66",
    "jszip": "3.10.0",
    "mitt": "^3.0.0",
    "pinia": "^2.0.34",
    "vue": "^3.3.4",
    "vue-toastification": "^2.0.0-rc.5",
    "vuetify": "^3.1.14",
    "whatwg-url": "^12.0.1",
    "wslink": "^1.6.4",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.12",
    "@rollup/plugin-replace": "^5.0.2",
    "@sentry/vite-plugin": "^2.4.0",
    "@types/chai": "^4.3.1",
    "@types/chai-almost": "^1.0.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-subset": "^1.3.3",
    "@types/deep-equal": "^1.0.1",
    "@types/file-saver": "^2.0.5",
    "@types/mocha": "^9.1.1",
    "@types/sinon": "^10.0.11",
    "@types/sinon-chai": "^3.2.8",
    "@types/whatwg-url": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.3.2",
    "@wdio/cli": "^8.11.2",
    "@wdio/local-runner": "^8.11.2",
    "@wdio/mocha-framework": "^8.11.0",
    "@wdio/spec-reporter": "^8.11.2",
    "@wdio/static-server-service": "^8.11.0",
    "babel-eslint": "10.1.0",
    "canvas": "^2.11.2",
    "chai": "4.3.6",
    "chai-almost": "^1.0.1",
    "chai-as-promised": "7.1.1",
    "chai-subset": "^1.6.0",
    "chromedriver": "^114.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-vue": "^8.0.3",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "geckodriver": "^4.0.4",
    "kw-doc": "3.1.2",
    "lint-staged": "10.5.4",
    "msedgedriver": "^91.0.0",
    "patch-package": "^6.4.7",
    "prettier": "2.8.8",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.62.0",
    "sinon": "14.0.0",
    "sinon-chai": "3.7.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-static-copy": "^0.16.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.32.1",
    "vue-tsc": "^1.8.8",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-edgedriver-service": "^3.0.2",
    "wdio-geckodriver-service": "^5.0.1",
    "wdio-html-nice-reporter": "^8.1.0",
    "wdio-image-comparison-service": "^5.0.3",
    "wdio-safaridriver-service": "^2.1.0",
    "wdio-wait-for": "^3.0.4",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint"
    ]
  }
}